{
  "name": "snipp.gg",
  "version": "1.0.0",
  "description": "An api wrapper of snipp.gg",
  "keywords": [
    "snipp",
    "uploads",
    "image",
    "sharing"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "lint": "prettier . --check",
    "format": "prettier --write .",
    "docs": "typedoc --out ../../docs --plugin typedoc-github-theme --cname lrclib.js.org",
    "size": "size-limit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run docs"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@eslint/js": "^9.37.0",
    "@types/jest": "^30.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-coverage-badges": "^1.0.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "author": "Igor Figueiredo",
  "license": "MIT"
}
